@using Ctf4e.Server.Controllers
@{
    ViewData["Title"] = "Praktika verwalten";

    var viewType = (AdminLabsController.ViewType)ViewData["ViewType"];
}

@if(viewType == AdminLabsController.ViewType.List)
{
    <partial name="AdminLabs.List.cshtml" />
}
else if(viewType == AdminLabsController.ViewType.Edit)
{
    <partial name="AdminLabs.Edit.cshtml" />
}
else if(viewType == AdminLabsController.ViewType.Create)
{
    <partial name="AdminLabs.Create.cshtml" />
}