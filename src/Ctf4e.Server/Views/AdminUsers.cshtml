@using Ctf4e.Server.Controllers
@{
    ViewData["Title"] = "Benutzerverwaltung";

    var viewType = (AdminUsersController.ViewType)ViewData["ViewType"];
}

@if(viewType == AdminUsersController.ViewType.List)
{
    <partial name="AdminUsers.List.cshtml" />
}
else if(viewType == AdminUsersController.ViewType.Edit)
{
    <partial name="AdminUsers.Edit.cshtml" />
}