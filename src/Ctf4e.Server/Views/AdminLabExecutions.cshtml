@using Ctf4e.Server.Controllers
@{
    ViewData["Title"] = "Ausführungen verwalten";

    var viewType = (AdminLabExecutionsController.ViewType)ViewData["ViewType"];
}

@if(viewType == AdminLabExecutionsController.ViewType.List)
{
    <partial name="AdminLabExecutions.List.cshtml" />
}
else if(viewType == AdminLabExecutionsController.ViewType.Edit)
{
    <partial name="AdminLabExecutions.Edit.cshtml" />
}
else if(viewType == AdminLabExecutionsController.ViewType.CreateForGroup)
{
    <partial name="AdminLabExecutions.CreateForGroup.cshtml" />
}
else if(viewType == AdminLabExecutionsController.ViewType.CreateForSlot)
{
    <partial name="AdminLabExecutions.CreateForSlot.cshtml" />
}

@section ScriptsHead
{
    <link href="~/lib/flatpickr/css/flatpickr.min.css" rel="stylesheet" />
    <script src="~/lib/flatpickr/js/flatpickr.min.js"></script>
    <script src="~/lib/flatpickr/js/l10n/de.js"></script>
}